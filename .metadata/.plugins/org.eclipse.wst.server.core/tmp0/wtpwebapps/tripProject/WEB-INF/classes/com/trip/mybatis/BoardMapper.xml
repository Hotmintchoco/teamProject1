<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.trip.mybatis.BoardMethod">

  <select id="selectOne" resultType="com.ezen.mybatis.BoardVO" parameterType="int">
    select * from post where num = #{num}
  </select>
  <select id="selectAllMember" resultType="com.trip.board.BoardVO">
    select * from post
  </select>
  <delete id="deleteBoard" parameterType="int">
    delete from post where num = #{num}
  </delete>
  <insert id="insertBoard" parameterType="com.trip.board.BoardVO">
  	insert into post values(#{num}, #{title}, #{content}, #{pictureurl})
  </insert>
  <update id="updateBoard" parameterType="com.trip.board.BoardVO">
  	update post set 
  	title = #{title}, content = #{content}, pictureurl = #{pictureurl}
  	where num = #{num}
  </update>
</mapper>